{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./out",
    "sourceMap": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}

